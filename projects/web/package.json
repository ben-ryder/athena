{
  "name": "@ben-ryder/athena-web",
  "description": "A local-first web app for notes, tasks, journaling, habit tracking and reminders.",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "sb:start": "storybook dev -p 6006 --no-open",
    "sb:build": "storybook build -o build",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest '.*.unit.test.ts' --detectOpenHandles --forceExit --runInBand --detectOpenHandles --forceExit",
    "test:e2e": "cypress run --e2e -q",
    "cy:start": "cypress open -b firefox",
    "cy:start:unit": "cypress open --component -b firefox",
    "cy:start:e2e": "cypress open --e2e -b firefox",
    "code:lint": "npx eslint .",
    "code:format": "npx prettier . --write",
    "generate-initial-change": "ts-node --esm scripts/generate-initial-change.ts"
  },
  "dependencies": {
    "@automerge/automerge": "^2.0.2",
    "@ben-ryder/jigsaw": "^0.17.1",
    "@ben-ryder/jigsaw-react": "^0.18.1",
    "@ben-ryder/lfb-common": "^0.5.0",
    "@ben-ryder/lfb-toolkit": "^0.5.0",
    "@codemirror/lang-markdown": "^6.1.0",
    "@codemirror/language-data": "^6.2.0",
    "@codemirror/view": "^6.9.4",
    "@hookform/resolvers": "^3.0.1",
    "@reduxjs/toolkit": "^1.9.3",
    "@uiw/codemirror-extensions-basic-setup": "^4.20.2",
    "@uiw/codemirror-extensions-hyper-link": "^4.19.16",
    "@uiw/codemirror-themes": "^4.19.16",
    "@uiw/react-codemirror": "^4.20.2",
    "classnames": "^2.3.2",
    "codemirror": "^5.65.13",
    "dexie": "^3.2.3",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "sass": "^1.62.0",
    "socket.io-client": "^4.6.1",
    "uuid": "^9.0.0",
    "validator": "^13.9.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^7.0.3",
    "@storybook/addon-essentials": "^7.0.3",
    "@storybook/addon-interactions": "^7.0.3",
    "@storybook/addon-links": "^7.0.3",
    "@storybook/addons": "^7.0.3",
    "@storybook/blocks": "^7.0.3",
    "@storybook/react": "^7.0.3",
    "@storybook/react-vite": "^7.0.3",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/testing-react": "^2.0.0",
    "@storybook/theming": "^7.0.3",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^3.1.0",
    "cypress": "^12.9.0",
    "eslint": "^8.44.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "3.0.0",
    "storybook": "^7.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-pwa": "^0.14.4",
    "vite-plugin-top-level-await": "^1.3.0",
    "vite-plugin-wasm": "^3.2.2"
  }
}
