{
  "name": "@ben-ryder/athena-web",
  "description": "The React web frontend for Athena.",
  "private": true,
  "version": "0.1.0-alpha",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "vite:dev-force": "vite --force",
    "vite:clean": "rm -rf node_modules/.vite",
    "sb": "start-storybook -p 6006",
    "sb:build": "build-storybook",
    "cy": "cypress open -b firefox",
    "cy:unit": "cypress open --component -b firefox",
    "cy:e2e": "cypress open --e2e -b firefox",
    "test": "npm run test:cy-unit && npm run test:cy-e2e",
    "test:cy-e2e": "cypress run --e2e -q",
    "test:cy-unit": "cypress run --component -q"
  },
  "dependencies": {
    "@automerge/automerge": "^2.0.0-beta.4",
    "@ben-ryder/jigsaw": "^0.7.0",
    "@ben-ryder/lfb-common": "^0.2.0",
    "@ben-ryder/lfb-toolkit": "^0.4.1",
    "@codemirror/lang-markdown": "^6.0.4",
    "@codemirror/language-data": "^6.1.0",
    "@codemirror/view": "^6.4.0",
    "@headlessui-float/react": "^0.10.0",
    "@headlessui/react": "^1.7.4",
    "@hookform/resolvers": "^2.9.10",
    "@logto/react": "^1.0.0-beta.13",
    "@reduxjs/toolkit": "^1.9.0",
    "@uiw/codemirror-extensions-hyper-link": "^4.12.4",
    "@uiw/codemirror-themes": "^4.12.4",
    "@uiw/react-codemirror": "^4.12.4",
    "classnames": "^2.3.2",
    "lucide-react": "^0.98.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.39.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "uuid": "^9.0.0",
    "validator": "^13.7.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@storybook/addon-a11y": "^6.5.13",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/testing-react": "^1.3.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "cypress": "^10.11.0",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.2",
    "typescript": "4.8.4",
    "vite": "^3.2.2",
    "vite-plugin-top-level-await": "^1.2.1",
    "vite-plugin-wasm": "^2.1.0"
  }
}
